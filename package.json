{
  "name": "my-ui-starter",
  "version": "0.0.1",
  "description": "It's a UI starter from make next developoment faster",
  "main": "index.js",
  "repository": "git@github.com:HamzaAmar/my-ui-starter.git",
  "author": "Hamza Miloud Amar <hamzamiloudamar@gmail.com>",
  "license": "MIT",
  "scripts": {
    "lint": "eslint src",
    "type-check": "tsc --noEmit",
    "type-check:watch": "yarn type-check -- --watch",
    "lint:fix": "yarn lint --fix",
    "lint:check": "yarn lint --quiet",
    "prettier:base": "prettier --parser typescript --single-quote",
    "prettier:check": "yarn prettier:base -- --list-different \"src/**/*.{ts,tsx}\"",
    "prettier:write": "yarn prettier:base -- --write \"src/**/*.{ts,tsx}\"",
    "validation": "yarn type-check && yarn lint:check && yarn prettier:check",
    "prepare": "husky install",
    "pre-commit": "lint-staged",
    "cz": "exec < /dev/tty && git cz --hook || true",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "devDependencies": {
    "@babel/core": "^7.18.13",
    "@commitlint/cli": "^17.0.3",
    "@commitlint/config-conventional": "^17.0.3",
    "@storybook/addon-actions": "^6.5.10",
    "@storybook/addon-essentials": "^6.5.10",
    "@storybook/addon-interactions": "^6.5.10",
    "@storybook/addon-links": "^6.5.10",
    "@storybook/builder-webpack5": "^6.5.10",
    "@storybook/manager-webpack5": "^6.5.10",
    "@storybook/preset-scss": "^1.0.3",
    "@storybook/react": "^6.5.10",
    "@storybook/testing-library": "^0.0.13",
    "@types/react": "^18.0.17",
    "@types/react-dom": "^18.0.6",
    "@typescript-eslint/eslint-plugin": "^5.35.1",
    "@typescript-eslint/parser": "^5.35.1",
    "babel-loader": "^8.2.5",
    "commitizen": "^4.2.5",
    "css-loader": "^6.7.1",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "8.2.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "2.25.3",
    "eslint-plugin-jsx-a11y": "6.5.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "7.28.0",
    "eslint-plugin-react-hooks": "4.3.0",
    "eslint-plugin-testing-library": "^5.6.0",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "sass": "^1.54.5",
    "sass-loader": "^13.0.2",
    "style-loader": "^3.3.1",
    "typescript": "^4.7.4"
  },
  "dependencies": {},
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
